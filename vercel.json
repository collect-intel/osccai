{
  "functions": {
    "api/cron/update-gac-scores.py": {
      "runtime": "@vercel/python@3.9"
    }
  },
  "routes": [
    {
      "src": "/api/cron/update-gac-scores",
      "dest": "python_api/cron/update-gac-scores.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/update-gac-scores",
      "schedule": "* * * * *"
    }
  ]
}
