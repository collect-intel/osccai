{
  "projects": [
    {
      "src": "app/",
      "use": "@vercel/next"
    },
    {
      "src": "api/",
      "use": "@vercel/python"
    }
  ],
  "functions": {
    "api/update-gac-scores.py": {
      "memory": 512,
      "maxDuration": 300
    }
  },
  "crons": [
    {
      "path": "/api/update-gac-scores.py",
      "schedule": "* * * * *"
    }
  ]
}
