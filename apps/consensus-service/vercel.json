{
  "version": 2,
  "builds": [
    {
      "src": "api/update-gac-scores.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/update-gac-scores",
      "dest": "api/update-gac-scores.py"
    }
  ],
  "crons": [
    {
      "path": "/api/update-gac-scores",
      "schedule": "* * * * *"
    }
  ],
  "buildCommand": "pip install -r requirements.txt"
}