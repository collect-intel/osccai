{
  "name": "osccai-web",
  "version": "0.1.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:local": "dotenv -e .env.local -- next dev",
    "dev:prod": "dotenv -e .env.production -- next dev",
    "build": "prisma generate && next build",
    "build:local": "dotenv -e .env.local -- prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:local": "dotenv -e .env.local -- prisma",
    "db:reset": "dotenv -e .env.local -- npx prisma db push --force-reset",
    "db:reset:prod": "read -p \"Are you sure you want to reset the production database? (y/n)\" -n 1 -r && echo && if [[ $REPLY =~ ^[Yy]$ ]]; then dotenv -e .env.production -- npx prisma db push --force-reset; fi",
    "db:migrate": "dotenv -e .env.local -- prisma migrate dev --name dev",
    "db:migrate:prod": "read -p \"Are you sure you want to migrate the production database? (y/n)\" -n 1 -r && echo && if [[ $REPLY =~ ^[Yy]$ ]]; then dotenv -e .env.production -- prisma migrate deploy; fi",
    "prisma:studio:prod": "dotenv -e .env.production -- prisma studio",
    "prisma:studio:local": "dotenv -e .env.local -- prisma studio",
    "cron:local": "pip install -r api/requirements.txt && dotenv -e .env.local -- node --loader ts-node/esm scripts/local-cron.ts",
    "db:shell:local": "dotenv -e .env.local -- psql \"$DATABASE_URL\"",
    "db:shell:prod": "read -p \"Are you sure you want to open a shell to the production database? (y/n)\" -n 1 -r && echo && if [[ $REPLY =~ ^[Yy]$ ]]; then dotenv -e .env.production -- psql \"$DATABASE_URL\"; fi"
  },
  "dependencies": {
    "@ai-sdk/openai": "^0.0.54",
    "@anthropic-ai/sdk": "^0.27.0",
    "@aws-sdk/client-s3": "^3.665.0",
    "@aws-sdk/s3-request-presigner": "^3.665.0",
    "@clerk/nextjs": "^5.5.2",
    "@headlessui/react": "^2.1.2",
    "@heroicons/react": "^2.1.5",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "^5.21.0",
    "@supabase/ssr": "^0.5.0",
    "@supabase/supabase-js": "^2.45.1",
    "@tailwindcss/forms": "^0.5.7",
    "@types/formidable": "^3.4.5",
    "Prettier": "npm:prettier@^3.3.3",
    "ai": "^3.3.20",
    "clsx": "^2.1.1",
    "csv-stringify": "^6.5.1",
    "date-fns": "^4.1.0",
    "formidable": "^3.5.1",
    "framer-motion": "^11.5.4",
    "lodash": "^4.17.21",
    "ml-kmeans": "^6.0.0",
    "ml-matrix": "^6.10.4",
    "ml-pca": "^4.0.0",
    "next": "14.2.5",
    "pino": "^9.4.0",
    "react": "^18",
    "react-dom": "^18",
    "react-dropzone": "^14.2.9",
    "react-icons": "^5.3.0",
    "react-markdown": "^9.0.1",
    "slugify": "^1.6.6",
    "uuid": "^10.0.0",
    "xmllm": "^0.0.15",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.15",
    "@types/lodash": "^4.17.9",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^10.0.0",
    "@vercel/node": "^2.0.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prisma": "^5.21.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
